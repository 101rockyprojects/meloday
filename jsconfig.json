{
  "compilerOptions": {
    "moduleResolution": "Bundler",
    "target": "ESNext",
    "module": "ESNext",
    "verbatimModuleSyntax": true,
    "isolatedModules": true,
    "resolveJsonModule": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "checkJs": false
  },
  "include": ["./**/*.d.ts", "./**/*.js", "./**/*.svelte", "public/**/*"],
}
